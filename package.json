    "ci": "npm build && npm test && npm lint && npm check"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageReporters": [
        "text",
        "cobertura"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blacksnow2k/azbuka.git"
  },
  "keywords": [
    "azbuka",
    "alphabet",
    "symbols"
  ],
  "author": "Leonid Perlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blacksnow2k/azbuka/issues"
  },
  "homepage": "https://github.com/blacksnow2k/azbuka#readme",
  "dependencies": {
    "@tensorflow/tfjs": "^4.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "parcel": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "types": "dist/index.d.ts"
}
